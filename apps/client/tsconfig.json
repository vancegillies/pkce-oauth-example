{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "../../.moon/cache/types/apps/client",
    "paths": {
      "pkce": [
        "../../packages/pkce/index.ts"
      ],
      "pkce/*": [
        "../../packages/pkce/*"
      ]
    }
  },
  "include": [
    "../../packages/pkce/**/*",
    "src"
  ],
  "references": [
    {
      "path": "../../packages/pkce"
    }
  ]
}
